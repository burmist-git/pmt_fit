\section{Fit function of the gain}
%------------------------------------------------
\begin{frame}
\frametitle{Fit function of the gain}
$\mathrm{Gain} = \frac{a^n}{(n+1)^{k*n}} * V^{k*n}$
\begin{table}
\begin{tabular}{l l l l}
\toprule
\textbf{PMT} & \textbf{n} & \textbf{a} & \textbf{k} \\
\midrule
R7378A BA1511 & 10 &\\
R7378A BA1512 & 10 &\\
R7378A BA1513 & 10 &\\
R7378A ZN2207 & 10 &\\
R762 NA0138   & 10 &\\
\bottomrule
\end{tabular}
\caption{Table of gains}
\end{table}
\end{frame}
